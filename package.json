{
  "name": "unified-deployment-orchestrator",
  "version": "1.0.0",
  "description": "Unified Deployment Orchestrator for Vercel, Railway, and Neon",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "cli": "ts-node src/index.ts",
    "server": "ts-node server/index.ts",
    "server:dev": "nodemon --watch server --ext ts --exec ts-node server/index.ts",
    "frontend:dev": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run server:dev\" \"npm run frontend:dev\" --names \"SERVER,FRONTEND\" --prefix-colors \"blue,green\""
  },
  "keywords": [
    "deployment",
    "orchestrator",
    "vercel",
    "railway",
    "neon"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vercel/sdk": "^1.2.3",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "fs-extra": "^11.2.0",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
